{
    "name": "plain-php-api",
    "description": "Plain PHP API skeleton with MySQL (PDO), AWS S3, and access token authentication",
    "type": "project",
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "files": [
            "classes/Database.php",
            "classes/Auth.php",
            "classes/Response.php",
            "classes/Router.php",
            "classes/S3Service.php"
        ]
    },
    "config": {
        "optimize-autoloader": true
    }
}
